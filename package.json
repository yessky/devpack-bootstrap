{
  "name": "devpack-bootstrap",
  "displayName": "Dev Pack Installer",
  "description": "Auto install popular lint related npm modules for development",
  "keywords": [
    "eslint",
    "devpack",
    "qakit",
    "boot"
  ],
  "version": "0.0.3",
  "publisher": "yessky",
  "repository": {
    "url": "https://github.com/yessky/devpack-bootstrap"
  },
  "bugs": {
    "url": "https://github.com/yessky/devpack-bootstrap/issues"
  },
  "homepage": "https://github.com/yessky/devpack-bootstrap",
  "icon": "devpack.png",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devpack.BootFix",
        "title": "Dev Pack: Reboot devpack to fix problems"
      },
      {
        "command": "devpack.QAKitFix",
        "title": "Dev Pack: Fix QAKit problems"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.66.0",
    "eslint": "^8.41.0"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3"
  }
}
